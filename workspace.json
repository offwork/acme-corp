{
  "version": 1,
  "projects": {
    "bookshop": {
      "root": "apps/bookshop",
      "sourceRoot": "apps/bookshop/src",
      "projectType": "application",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/web:build",
          "options": {
            "outputPath": "dist/apps/bookshop",
            "index": "apps/bookshop/src/index.html",
            "main": "apps/bookshop/src/main.tsx",
            "polyfills": "apps/bookshop/src/polyfills.ts",
            "tsConfig": "apps/bookshop/tsconfig.app.json",
            "assets": [
              "apps/bookshop/src/favicon.ico",
              "apps/bookshop/src/assets"
            ],
            "styles": [
              "libs/ui/src/lib/ui.scss",
              "apps/bookshop/src/styles.scss"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/bookshop/src/environments/environment.ts",
                  "with": "apps/bookshop/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "bookshop:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "bookshop:build:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/bookshop/.eslintrc",
            "tsConfig": [
              "apps/bookshop/tsconfig.app.json",
              "apps/bookshop/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/bookshop/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/bookshop/jest.config.js",
            "tsConfig": "apps/bookshop/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "bookshop-e2e": {
      "root": "apps/bookshop-e2e",
      "sourceRoot": "apps/bookshop-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/bookshop-e2e/cypress.json",
            "tsConfig": "apps/bookshop-e2e/tsconfig.e2e.json",
            "devServerTarget": "bookshop:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "bookshop:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "apps/bookshop-e2e/.eslintrc",
            "tsConfig": ["apps/bookshop-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/bookshop-e2e/**"]
          }
        }
      }
    },
    "ui": {
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/ui/.eslintrc",
            "tsConfig": [
              "libs/ui/tsconfig.lib.json",
              "libs/ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/ui/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "tsConfig": "libs/ui/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "books-feature": {
      "root": "libs/books/feature",
      "sourceRoot": "libs/books/feature/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "config": "libs/books/feature/.eslintrc",
            "tsConfig": [
              "libs/books/feature/tsconfig.lib.json",
              "libs/books/feature/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/books/feature/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/books/feature/jest.config.js",
            "tsConfig": "libs/books/feature/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    },
    "books-data-access": {
      "root": "libs/books/data-access",
      "sourceRoot": "libs/books/data-access/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/books/data-access/tsconfig.lib.json",
              "libs/books/data-access/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/books/data-access/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/books/data-access/jest.config.js",
            "tsConfig": "libs/books/data-access/tsconfig.spec.json",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "bookshop"
}
